<launch>

    <!--- Run Rviz-->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find lego_loam)/launch/test.rviz" />

    <!--- TF -->
    <node pkg="tf" type="static_transform_publisher" name="camera_init_to_map"  args="0 0 0 0 0 0 /map /camera_init 10" />
    <node pkg="tf" type="static_transform_publisher" name="base_link_to_camera" args="0 0 0 -1.570792 -1.570792 0 /camera /base_link1 10" />
    <!--node pkg="tf" type="static_transform_publisher" name="world_to_base_link" args="0 0 0 0 0 0 /base_link /imu_gc 10" /-->

</launch>
